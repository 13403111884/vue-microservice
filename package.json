{
  "name": "vue-microservice",
  "version": "1.0.0",
  "description": "用`qiankun`来实现`vue`技术栈的前端微服务",
  "main": "index.js",
  "scripts": {
    "install:hash": "cd app-vue-hash && npm --registry https://registry.npm.taobao.org install",
    "install:history": "cd app-vue-history && npm --registry https://registry.npm.taobao.org install",
    "install:main": "cd main && npm --registry https://registry.npm.taobao.org install",
    "install": "npm-run-all --serial install:*",
    "start:hash": "cd app-vue-hash && npm run serve ",
    "start:history": "cd app-vue-history && npm run serve",
    "start:main": "cd main && npm run serve",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel start:*",
    "serve": "cross-env NODE_ENV=development npm-run-all --parallel start:*",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel start:*",
    "build:hash": "cd app-vue-hash && npm run build",
    "build:history": "cd app-vue-history && npm run build",
    "build:main": "cd main && npm run build",
    "build": "npm-run-all --parallel build:*"
  },
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
